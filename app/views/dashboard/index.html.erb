<h1>Dashboard</h1>

<p>Welcome, <%= current_user.email %>!</p>

<ul>
  <% @posts.each do |post| %>
  <li>
      <a href="/posts/<%= post.id %>">
      <%= post.title %>
      </a>
  </li>
  <% end %>
</ul>
<%= button_to 'Sign Out', destroy_user_session_path, method: :delete %>



<h2>Create a New Post</h2>
<%= link_to 'New Post', new_post_path, class: 'btn btn-primary' %>
